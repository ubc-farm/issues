<script marko-init>
	var ic = require('../../_helpers/icon.js');
</script>
<macro mapButton(icon, title)>
	<button id="map-${icon}" title="${title}" aria-label="${title}">
		<img src="${ic.format(icon)}" alt=""/>	
	</button>	
</macro>

<menu class="js">
	<mapButton icon="map-locate"/>
	<mapButton icon="map-zoom-in"/>
	<mapButton icon="map-zoom-out"/>
	<mapButton icon="map-street-view"/>
</menu>