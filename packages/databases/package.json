{
	"name": "@ubc-farm/databases",
	"version": "2.0.0",
	"module": "index.mjs",
	"main": "index.js",
	"dependencies": {
		"pouchdb": "^6.1.2",
		"pouchdb-find": "^0.10.5",
		"transform-pouch": "^1.1.3",
		"moment": "^2.17.1",
		"lodash": "^4.17.4"
	},
	"devDependencies": {
		"rollup": "^0.41.4",
		"typescript": "^2.2.1",

		"shortid": "^2.2.6",
		"docuri": "^4.2.2",
		"@turf/area": "^3.7.5",
		"@turf/centroid": "^3.7.5",
		"dts-generator": "^2.1.0",
		"phone": "^1.0.9"
	},
	"optionalDependencies": {
		"react": "^15.4.2"
	},
	"scripts": {
		"prepare:umd": "rollup -c -i src/index.ts -o index.js -f umd",
		"prepare:es": "rollup -c -i src/index.ts -o index.mjs -f es",
		"dts": "dts-generator --project . --name databases --out index.d.ts",
		"prepare": "npm run prepare:es && npm run prepare:umd",
		"prepublish": "npm run prepare",
		"ts": "tsc --noEmit || exit 0"
	}
}
