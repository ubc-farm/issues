{
	"name": "@ubc-farm/fields",
	"version": "1.0.0",
	"dependencies": {
		"document-promises": "^3.0.0",
		"react-virtualized": "^8.11.4",
		"lodash": "^4.17.4",
		"@ubc-farm/databases": "^2.0.0",
		"@ubc-farm/map-utils": "^1.0.0"
	},
	"peerDependencies": {
		"react": "^15.4.2",
		"react-dom": "^15.4.2"
	},
	"devDependencies": {
		"rollup": "^0.41.4",
		"typescript": "^2.2.1"
	},
	"scripts": {
		"build-index": "rollup -c -i entry/index.ts -o browser/index.js",
