{
  "name": "@ubc-farm/lookup-service",
  "version": "1.0.1",
  "description": "API for looking up static information about plants grown at the farm",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "author": "Tiger Oakes",
  "license": "MIT",
  "repository": "ubc-farm/lookup-service",
  "dependencies": {
    "joi": "^9.2.0",
    "node-fetch": "^1.6.3"
  },
  "devDependencies": {
    "@types/joi": "^9.0.32",
    "@types/node-fetch": "^1.6.5",
    "@ubc-farm/server": "^2.3.1"
  },
  "peerDependencies": {
    "@ubc-farm/server": "^2.3.1"
  },
  "scripts": {
    "prepare": "rollup -c && rollup src/plugin.js -c -o dist/plugin.js -f cjs",
    "prepublish": "npm run prepare",
    "start": "ubc-farm-server ./package.json"
  },
  "ubc-farm": {
    "server-plugin": {
      "register": "dist/plugin.js",
      "routes": {
        "prefix": "/lookup"
      }
    }
  }
}
