{
  "name": "@ubc-farm/lookup-service",
  "version": "1.0.0",
  "description": "API for looking up static information about plants grown at the farm",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "author": "Tiger Oakes",
  "license": "MIT",
  "repository": "ubc-farm/lookup-service",
  "dependencies": {
    "joi": "^9.2.0",
    "node-fetch": "^1.6.3"
  },
  "devDependencies": {
    "@types/joi": "^9.0.32",
    "@types/node-fetch": "^1.6.5",
    "@ubc-farm/eslint-config": "ubc-farm/eslint-config",
    "eslint": "^3.8.1",
    "eslint-plugin-import": "^2.0.1",
    "rollup": "^0.36.3"
  },
  "peerDependencies": {
    "@ubc-farm/server": "ubc-farm/server"
  },
  "scripts": {
    "prepare": "rollup -c && rollup src/plugin.js -c -o dist/plugin.js -f cjs",
    "start": "ubc-farm-server ."
  },
  "ubc-farm": {
    "server-plugin": {
      "register": "dist/plugin.js",
      "routes": { "prefix": "/lookup" }
    }
  }
}
