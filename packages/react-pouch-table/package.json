{
	"name": "@ubc-farm/react-pouch-table",
	"version": "1.0.0",
	"module": "index.mjs",
	"main": "index.js",
	"dependencies": {
		"react-virtualized": "^9.1.0",
		"@ubc-farm/databases": "^2.0.0"
	},
	"devDependencies": {
		"rollup": "^0.41.4",
		"typescript": "^2.2.1"
	},
	"peerDependencies": {
		"react": "^15.4.2"
	},
	"scripts": {
		"ts": "tsc --noEmit || exit 0",
		"build:es": "rollup -c -f es -i src/ReactPouchTable.tsx -o index.mjs",
		"build:cjs": "rollup -c -f cjs -i src/ReactPouchTable.tsx -o index.js",
		"prepare": "npm run build:es && npm run build:cjs",
		"prepublish": "npm run prepare"
	}
}
